extends layout

block content
    h1 3D Modeling
    if user
        .upload-container
            a(href='./modeling/upload')
                button.btn.btn-outline-primary(type='button') Upload
        each val in files  
            ul
                li
                    img(src=`/modeling/image/${val._id}`)
    else
        each val in files  
            ul
                li
                    img(src=`/modeling/image/${val._id}`)