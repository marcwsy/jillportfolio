extends layout

block content
    h1 Illustration
    if user
        .upload-container
            a(href='./illustration/upload')
                button.btn.btn-outline-primary(type='button') Upload
        each val in files  
            ul
                li
                    img(src=`/illustration/image/${val._id}`)
    else
        each val in files  
            ul
                li
                    img(src=`/illustration/image/${val._id}`)